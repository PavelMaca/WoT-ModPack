<config>
	<!-- Статистика после боя -->

	<!-- 0 - Сортировка по урону, 1 - по Опыту 	-->

	<teamSort type="0"/>

	<!-- Достижения -->

	<!-- Цифры - количество знаков после запятой, цифра 0 - не выводится ничего -->

	<!-- фраги -->

	<frags coeff=		"2"	/>

	<!-- средний урон -->

	<damageDealt coeff=	"4"	/>

	<!-- победы -->

	<wins coeff=		"3"	/>

	<!-- выживаемость -->

	<survivedBattles coeff=	"0"	/>

	<!-- средний опыт -->

	<avgExperience coeff=	"2"	/>



  <!--  
	Типы сортировок:

		descend="1" - по убыванию
		descend="0" - по возрастанию

	Поля для сортировки:

		fights - количество боев
		wins   - процент побед	
		name   - имя
		level  - уровень
		nation - нация
		type   - тип танка

  --> 

  <sorts>
	<!-- По боям -->

	<s1 name="Bitev">
		<f1 field="fights" 	descend="1"/>  
	</s1>

	<!-- По проценту побед -->

	<s2 name="Vítězství">
		<f1 field="wins" 	descend="1"/>
	</s2>

	<!-- Мастера -->

	<s3 name="VT">
		<f1 field="vehicleClass" descend="1"/>
		<f2 field="level"	 descend="1"/>
		<f3 field="name" 	 descend="0" />
	</s3>


	<!-- По Типу (втростепенные по типу танка, потом по имени) -->

	<s4 name="Typ">
		<f1 field="type" 	descend="1" />
		<f2 field="level"	descend="1"/>
		<f3 field="name" 	descend="0" /> 
	</s4>

	<!-- По Уровню (втростепенные по нации, потом по имени) -->

	<s5 name="Tier">

		<f1 field="level" 	descend="1" />
		<f2 field="nation" 	descend="0"/>
		<f3 field="name" 	descend="0" />
	</s5>

	<!-- По названию -->

	<s6 name="Název">
		<f1 field="name" 	descend="0"/>
	</s6>

  </sorts>
  
