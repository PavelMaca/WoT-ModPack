{
    "showStatForBattle": true,
    "battleStatPatterns": [
         {
            "pattern": "(Získáno kreditů: )<font color='#C5CFCF'>[\\d ]+",
            "repl": "\\1<font color='{{c:credits}}'>{{credits}}"
        },
        {
            "pattern": "(Získáno XP: )<font color='#C8C8B5'>[\\d ]+",
            "repl": "\\1<font color='{{g:avgPlace}}'>{{totalXP}}"
        },
        {
            "if": "{{avgXP}} != {{tmenXP}}",
            "pattern": "(Získáno XP: .*)",
            "repl": "\\1\n<font color='#929290'>XP posádky: {{tmenXP}}</font>"
        }
    ],
    "battleStatText": [
    "<font size='8'> </font>",
    "     <img src='img://gui/maps/icons/library/Assassik/Wrench.png' width='20' height='18' align='baseline' vspace='-14'> <font size='12' color='#F0DDB9'>Opravy:</font>  <font size='12' color='#FFFFFF'><b>{{autoRepair}}</b></font> <img src='img://gui/maps/icons/library/Assassik/CreditsIcon.png' width='16' height='16' align='baseline' vspace='-4'>",
    "     <img src='img://gui/maps/icons/library/Assassik/Shell.png' width='20' height='18' align='baseline' vspace='-14'> <font size='12' color='#F0DDB9'>Munice:</font>  <font size='12' color='#FFFFFF'><b>{{autoLoad}}</b></font> <img src='img://gui/maps/icons/library/Assassik/CreditsIcon.png' width='16' height='16' align='baseline' vspace='-4'>",
    "     <img src='img://gui/maps/icons/library/Assassik/Toolbox.png' width='20' height='18' align='baseline' vspace='-14'> <font size='12' color='#F0DDB9'>Spotř. mat.:</font>  <font size='12' color='#FFFFFF'><b>{{autoEquip}}</b></font> <img src='img://gui/maps/icons/library/Assassik/CreditsIcon.png' width='16' height='16' align='baseline' vspace='-4'>",
    "<font size='8'> </font>",
    "<textformat tabstops='[105]'><font size='13' color='#F0DDB9'><b>Celkem: <font color='{{c:credits}}'>{{credits}}</font> <img src='img://gui/maps/icons/library/Assassik/CreditsIcon.png' width='16' height='16' align='baseline' vspace='-4'><tab>WN8: <font color='{{c:WN8}}'>{{WN8}}</font> <font color='{{c:XWN8}}'>({{XWN8}})</font></b></font></textformat>"
