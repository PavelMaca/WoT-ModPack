<crew_extended.xml>
	<setup>
		<module_crew_extended>	true	</module_crew_extended>
		<debug>	false	</debug>
		<hangar_crew_info>	true	</hangar_crew_info>
		<barracks_crew_info>	true	</barracks_crew_info>
		<basic_training_level>	false	</basic_training_level>
		<language>	OTHER	</language>
		<hint>	support language RU,EN,DE,OTHER	</hint>
	</setup>
	<hangar_crew_info>
		<txt_hangar_firstname>	{firstname}	</txt_hangar_firstname>
		<txt_hangar_lastname>	{lastname}	</txt_hangar_lastname>
		<txt_hangar_rank>	{rank}	</txt_hangar_rank>
		<txt_hangar_colored_role>	<![CDATA[{exp-total}</img><img src="img://gui/maps/icons/library/multyXp.png" width="15" height="15" >{role}]]>	</txt_hangar_colored_role>
		<txt_hangar_colored_vehicletype>	{vehicleType}	</txt_hangar_colored_vehicletype>
		<hint>	support macros in txt: {training-level},{basic-training-level},{firstname},{lastname},{rank},{exp-total},{training-battles},{training-progress},{role},{CurExp},{vehicleType},{exp-step},{exp-step-battles},{exp-extra-level},{exp-free},{skillsCnt},{lastSkillLevel},{nxtskillvlv},{training-level-int},{exp-total-int},{training-battles-int},{nxtskillvlv-int},{skillsCnt-int},{training-progress-int}, {CurExp-int},{exp-step-int}, {exp-step-battles-int},{exp-extra-level-int},{exp-free-int},{lastSkillLevel-int}	</hint>
	</hangar_crew_info>
	<barracks_crew_info>
		<txt_barracks_firstname>	{firstname}	</txt_barracks_firstname>
		<txt_barracks_lastname>	{lastname}	</txt_barracks_lastname>
		<txt_barracks_rank>	{role}	</txt_barracks_rank>
		<txt_barracks_colored_role>	<![CDATA[{exp-total}</img><img src="img://gui/maps/icons/library/multyXp.png" width="15" height="15" >{role}]]>	</txt_barracks_colored_role>
		<txt_barracks_colored_vehicleType>	{vehicleType}	</txt_barracks_colored_vehicleType>
		<txt_barracks_lockMessage>	{lockMessage}	</txt_barracks_lockMessage>
		<hint>	support macros in txt: {skillicons},{lockMessage},{training-level},{firstname},{lastname},{rank},{exp-total},{training-battles},{training-progress},{role},{CurExp},{vehicleType},{exp-step},{exp-step-battles},{exp-extra-level},{exp-free},{skillsCnt},{lastSkillLevel},{nxtskillvlv},{training-level-int},{exp-total-int},{training-battles-int},{nxtskillvlv-int},{skillsCnt-int},{training-progress-int}, {CurExp-int},{exp-step-int}, {exp-step-battles-int},{exp-extra-level-int},{exp-free-int},{lastSkillLevel-int}	</hint>
	</barracks_crew_info>
	<other_language>
		<txt_premium_bonus_on>	Koeficient XP posádky za prémiový tank	</txt_premium_bonus_on>		<txt_premium_bonus_off></txt_premium_bonus_off>
		<txt_up_to_exp>	XP na	</txt_up_to_exp>
		<txt_up_to_battle>	Bitev na	</txt_up_to_battle>
		<txt_skill_number>	Skill	</txt_skill_number>
		<txt_basic_skill>	Výcvik	</txt_basic_skill>
		<txt_SilverReLearn>	Přeškolení	</txt_SilverReLearn>
		<txt_SilverReLearn_need>	za Kredity potřebuje:	</txt_SilverReLearn_need>
		<txt_SilverReLearn_ready>	za Kredity připraveno!	</txt_SilverReLearn_ready>
		<txt_SilverReLearn_notready>	za Kredity není připraveno	</txt_SilverReLearn_notready>
		<txt_SkillDrop>	Nová schopnost po zrušení 	</txt_SkillDrop>
		<txt_SkillDrop_ready>	Zrušení shopnosti za kredity připraveno!	</txt_SkillDrop_ready>
	</other_language>
</crew_extended.xml>
