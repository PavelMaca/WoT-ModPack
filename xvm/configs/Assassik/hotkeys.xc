/**
 * Special XVM hotkeys
 */
{
  "hotkeys": {
    // onHold: take action by key click or while key is remains pressed
    // Keycode values a be found here:
    // http://www.cambiaresearch.com/articles/15/javascript-char-codes-key-codes
    // Most keys are unable to be assigned.
    // Common working keys are Ctrl - 17, X - 88, C - 67
    "minimapZoom": { "enabled": true, "onHold": true, "keyCode": 17 }
  }
}
